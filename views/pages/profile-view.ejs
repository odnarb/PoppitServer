                        <div class="kt-portlet kt-portlet--mobile">
                            <div class="kt-portlet__head kt-portlet__head--lg">
                                <div class="kt-portlet__head-label">
                                    <img src="<%- vuser.profile_picture %>" width=150px title="<%- vuser.first_name %> <%- vuser.last_name %>" />
                                    <h3 class="kt-portlet__head-title">
                                        <%- vuser.first_name %> <%- vuser.last_name %>
                                    </h3>
                                </div>
                                <div class="kt-portlet__head-toolbar">
                                    <div class="kt-portlet__head-wrapper">
                                        <div class="kt-portlet__head-actions">
                                            <a href="/profile/edit/<%- vuser.id %>" class="btn btn-warning btn-elevate btn-icon-sm">
                                                <i class="la la-edit"></i>
                                                Edit Profile
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="kt-portlet__body">

                                <div class="kt-widget13">
                                    <% Object.keys(vuser).forEach( idx => { %>
                                    <div class="kt-widget13__item">
                                        <span class="kt-widget13__desc"><%- idx %></span>
                                        <span class="kt-widget13__text"><%- vuser[idx] %></span>
                                    </div>
                                    <% }) %>

                                </div>

                            </div>
                        </div>

